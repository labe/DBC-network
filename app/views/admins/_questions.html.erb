<span class="category-title">Questions</span><br>
<table class="admin_table">
  <thead>
    <tr>
      <th>Company</th>
      <th>User</th>
      <th>Question</th>
      <th>Answer</th>
    </tr>
  </thead>
  <tbody>
    <% answers.each do |answer| %>
    <tr>
      <th><%= answer.question.user.groupable.name %></th>
      <th><%= answer.user.id %></th>
      <th><%= answer.question.text %></th>
      <th><%= answer.text %></th>
    </tr>
    <% end %>
  </tbody>
</table>
