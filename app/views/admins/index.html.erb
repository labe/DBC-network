Students:<br>
<% @students.each do |student| %>
  Name: <%= student.first_name %> <%= student.last_name %>
  User id: <%= student.id %>
  Last login: <%= student.last_login %>
  Email: <%= student.email %>
  Status: <%= student.status %>
  <%= render partial: 'users/update_status_buttons', locals: { user: student } %>
  Cohort: <%= student.groupable.name %>
  <%= render partial: 'users/update_activation_buttons', locals: { user: student } %>
<br><br><br>
<% end %>

<br><br><br>

Employers:<br>
<% @employers.each do |employer| %>
  Name: <%= employer.first_name %> <%= employer.last_name %>
  Last login: <%= employer.last_login %>
  Email: <%= employer.email %>
  Status: <%= employer.status %>
  <%= render partial: 'users/update_status_buttons', locals: { user: employer } %>
  Part of: <%= employer.groupable.name %>
  <%= render partial: 'users/update_activation_buttons', locals: { user: employer } %>
<br><br><br>
<% end %>

<br><br><br>

Interests:<br>
<% @interests.each do |interest| %>
Who showed interest? <%= interest.pitcher.first_name %> <%= interest.pitcher.last_name %>
Who accepted interest? <%= interest.catcher.first_name %> <%= interest.catcher.last_name %>
Approved? <%= interest.approved %>
<br><br><br>
<% end %>

<br><br><br>

Companies:<br>
<% @companies.each do |company| %>
Name: <%= company.name %>
Website: <%= company.website %>
Logo: <%= company.logo %>
Location: <%= company.location %>
Status: <%= company.status %>
Initial contact: <%= company.initial_email_contact %>
<br><br><br>
<% end %>

<br><br><br>


Cohorts:<br>
<% @cohorts.each do |cohort| %>
Name: <%= cohort.name %>
<br><br><br>
<% end %>
