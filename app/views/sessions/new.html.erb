<%= image_tag "devbootcamp.png", :size => "206x97" %>
<%= form_tag("/users", :method => "post") do %>
  <%= text_field_tag :email, nil, :placeholder => "Email" %>
  <%= text_field_tag :password, nil, :placeholder => "Password" %>
  <%= submit_tag("Login") %>
<% end %>
<%= link_to "Request an Account", sign_up_path %>