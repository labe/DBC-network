<!DOCTYPE html>
<html>
<head>
  <title>DBConnect</title>
  <%= stylesheet_link_tag    "normalize", :media => "all" %>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<% unless current_page?(root_path) || current_page?(sign_up_path) || current_page?(new_student_path) %>
  <%= render "layouts/navbar" %>
  <%= render "layouts/content_nav" %>
<% end %>

<%= yield %>

<div class="error_flash">
  <% flash.each do |name, msg| %>
    <%= content_tag :div, msg, :id => "flash_#{name}" %>
  <% end %>
</div>


</body>
</html>
