<div>
  <h2>User List</h2>
  <ul class="user_list">
    <% @users.each do |user| %>
      
      <li id="<%=user.id%>"
          data-location="<%= user.location %>"
          data-cohort="<%= user.groupable.name %>"
          data-status="<%= user.status %>"
          data-last-login="<%= user.last_login %>"
      >
        <div class="user-div">
          <%= user.last_name %>, <%= user.first_name %>
        </div>
      </li>
    <% end %>
  </ul>
</div>
