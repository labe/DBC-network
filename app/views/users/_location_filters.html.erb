<div>
  <h3>Locations</h3>
  <button class="clear" >Clear Filters</button>
  <% @users.group('location').select('location').where("location IS NOT NULL").each do |user| %>
    <button class="location_filters" id="<%= user.location.gsub(/\s+/, "") %>"><%= user.location %></button>
  <% end %>
</div>
