<% if @user.status == "active" %>
  <% if @user.groupable_type == "Cohort" %>
    <%= render partial: 'users/student_profile', locals: { user: @user } %>
  <% elsif @user.groupable_type == "Company" %>
    <%= render partial: 'users/employer_profile', locals: { user: @user } %>
  <% end %>
<% elsif @user.status == "inactive" %>
  <span>This user is inactive.</span>
<% end %>

<div>
	<table id="github_list_showing" class="selected_repos">
		<tr>
			<td>Drag your top 5 repos that you are proud of for employers to see on your profile.</td>
		</tr>
	</table>
</div>

<table class="all_repos">
	<% @repos.each do |repo| %>
	<tr>
		<td><%= repo[0] %></td>
	</tr>
	<%end%>
</table>